function puti(x) {
  if x == 0 {
    puts("0");
    return 0;
  }
  var buffer[20];
  buffer[19] = 0;
  var i = 19;
  while x {
    i = i - 1;
    buffer[i] = '0' + x % 10;
    x = x / 10;
  }
  puts(buffer + i);
}
